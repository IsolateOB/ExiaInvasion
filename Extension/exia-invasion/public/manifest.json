{
  "name": "ExiaInvasion",
  "description": "A crawler program that obtains Nikke character data in personal accounts from blablalink and generates progress tracker",
  "version": "2.0",
  "manifest_version": 3,

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://https://www.blablalink.com/*"
      ]
    }
  ]
}